@model List<AppointmentServiceModel>

<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Physician</th>
            <th scope="col">Patient</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var appointment in Model)
        {
            <tr>
                <th scope="row">@appointment.Date</th>
                <td>@appointment.PhysicianName</td>
                <td>@appointment.PatientName</td>
                <td><a class="btn btn-primary" asp-area="" asp-controller="Physicians" asp-action="Details" asp-route-physicianId="@appointment.PhysicianId">Physician Details</a></td>
            </tr>
         }
    </tbody>
</table>
